{
  "version": 2,
  "functions": {
    "api/proposals/download/[filename].js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/proposals/download/(.*)",
      "destination": "/api/proposals/download/[filename]?filename=$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "source": "/api/proposals/download/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    }
  ]
}